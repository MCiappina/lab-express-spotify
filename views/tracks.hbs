<ul class="list-group">
    {{#each data}}
    <li class="list-group-item">
        <div>
            <h3>{{this.name}}</h3>
            <audio controls>
                <source src="{{this.preview_url}}" type="audio/mpeg">                
            </audio>
        </div>
    </li>
    {{/each}}
</ul>